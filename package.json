{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only --respawn -- src/index.ts | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test:ci": "npm run test:unit && npm run test:functional",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "engines": {
    "node": "16"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.13.7",
    "express-pino-logger": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^6.2.8",
    "pino": "^7.9.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
